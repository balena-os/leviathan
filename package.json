{
  "name": "resinos-tests",
  "version": "1.5.0",
  "description": "Automated testing for ResinOS",
  "scripts": {
    "lint": "eslint lib scripts",
    "test": "npm run lint",
    "start": "node lib/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io/resinos-tests.git"
  },
  "keywords": [
    "resinOS",
    "testing",
    "resin.io",
    "resin"
  ],
  "author": "Resin Inc. <hello@etcher.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io/resinos-tests/issues"
  },
  "homepage": "https://github.com/resin-io/resinos-tests",
  "ava": {
    "files": [
      "lib/main.js"
    ],
    "failWithoutAssertions": true,
    "serial": true,
    "concurrency": 1,
    "verbose": true
  },
  "dependencies": {
    "ava": "^0.23.0",
    "bluebird": "^3.5.0",
    "bluebird-retry": "^0.11.0",
    "byline": "^5.0.0",
    "drivelist": "^5.0.22",
    "etcher-image-write": "^9.1.3",
    "fs-extra": "^3.0.1",
    "inquirer": "^5.1.0",
    "make-runnable": "^1.3.6",
    "mountutils": "^1.2.0",
    "node-ipc": "^9.1.1",
    "progress-stream": "^2.0.0",
    "randomstring": "^1.1.5",
    "resin-cli-visuals": "^1.4.0",
    "resin-image-fs": "^4.0.2",
    "resin-sdk": "^8.1.0",
    "semver": "^5.5.0",
    "simple-git": "^1.85.0",
    "ssh-keygen": "^0.4.1"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsdoc": "^3.5.0",
    "eslint-plugin-lodash": "^2.6.1",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
